  <small-icon />
  <large-icon />
  <description>Scrive un messaggio</description>
  <attribute>
   <name>message</name>
   <required>true</required>
   <rtexprvalue>true</rtexprvalue>
  </attribute>
  <example />
 </tag>


</taglib>